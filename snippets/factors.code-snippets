      "\tvector<vector<long long>> w(n+1);",
      "\tfor (auto & i : v) {",
      "\t\tfor (long long j = i; j <= n; j += i) {",
      "\t\t\tw[j].push_back(i);",
      "\t\t}",
      "\t}",
